{
  "name": "project1-front",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "prod": "ng build --prod",
    "dev": "ng serve -o --ssl",
    "dev:prod": "ng serve -o --ssl --environment=prod",
    "dev:server": "ng serve -o --ssl --environment=local-server",
    "start": "node server.js",
    "build": "ng build",
    "test": "ng test --browsers=Chrome --sourcemaps=false",
    "test:once": "ng test --single-run --browsers=ChromeHeadless",
    "lint": "ng lint",
    "format:fix": "pretty-quick --staged",
    "precommit": "run-s format:fix lint test:once",
    "format:check":
      "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "e2e": "cypress open",
    "e2e:run": "cypress run",
    "test:ci": "ng test --single-run --browsers=ChromeHeadless",
    "lint:ci": "ng lint --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.9",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/forms": "^5.2.9",
    "@angular/http": "^5.2.9",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@angular/router": "^5.2.9",
    "angular2-csv": "^0.2.5",
    "angular4-datepicker": "^1.0.1",
    "auth0-js": "^9.4.2",
    "auth0-lock": "^11.5.2",
    "bootstrap": "^3.3.7",
    "core-js": "^2.5.3",
    "express": "^4.16.3",
    "hammerjs": "^2.0.8",
    "json2csv": "^3.11.5",
    "rxjs": "^5.5.8",
    "zone.js": "^0.8.25"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.3",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/language-service": "^5.2.9",
    "@bahmutov/add-typescript-to-cypress": "^2.0.0",
    "@cypress/snapshot": "^2.0.1",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.94",
    "angular-in-memory-web-api": "^0.5.4",
    "cypress": "^2.1.0",
    "cypress-cucumber-preprocessor": "^0.3.0",
    "husky": "^0.14.3",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.11.1",
    "pretty-quick": "^1.4.1",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  },
  "engines": {
    "node": "8.9.4",
    "npm": "5.6.0"
  }
}
